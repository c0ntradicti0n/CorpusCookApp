
<Annotation_Screen>:
    GridLayout:
        rows:5
        cols:1
        InfoLabel:
            halign: "center"
            valign: "middle"
            markup: True
            size_hint_y: 9.5
            text_size: self.size
            multiline: True
            id: sample
            text: "sample"
            font_size: 40
        NavButton:
            text: "Oll Korrect!"
            font_size: 30
            on_press: root.manager.ok()
        NavButton:
            text: "To hard for me. Put it in the one day maybe bin."
            font_size: 30
            on_press: root.manager.complicated_sample()
        NavButton:
            text: "Just no!"
            font_size: 30
            on_press: root.manager.shit()
        NavButton:
            text: "Manipulate"
            font_size: 30
            on_press: root.manager.manipulate()
